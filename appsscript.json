{
  "timeZone": "Asia/Tokyo",
  "dependencies": {},
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "urlFetchWhitelist": ["https://api.notion.com/"],
  "oauthScopes": [
    "https://www.googleapis.com/auth/calendar.addons.execute",
    "https://www.googleapis.com/auth/calendar.readonly",
    "https://www.googleapis.com/auth/script.locale",
    "https://www.googleapis.com/auth/script.external_request"
  ],
  "addOns": {
    "common": {
      "name": "My Calendar Add-on",
      "logoUrl": "https://raw.githubusercontent.com/kazscape/gcal-event-to-notion/main/icon.png"
    },
    "calendar": {
      "homepageTrigger": {
        "runFunction": "onHomepageOpen"
      },
      "eventOpenTrigger": {
        "runFunction": "onEventOpen"
      }
    }
  }
}
